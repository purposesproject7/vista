# Client Environment Configuration

# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_SOCKET_URL=http://localhost:5000

# WebSocket Configuration
VITE_WS_ENABLE_COMPRESSION=true
VITE_WS_MAX_RECONNECT_ATTEMPTS=5
VITE_WS_RECONNECT_DELAY=1000
VITE_WS_HEARTBEAT_INTERVAL=30000
VITE_WS_CONNECTION_TIMEOUT=10000

# Potato Device Optimizations
VITE_ENABLE_POTATO_MODE=auto
VITE_MAX_CACHE_SIZE=50
VITE_UPDATE_THROTTLE_MS=2000
VITE_COMPRESSION_THRESHOLD=1024

# Performance Settings
VITE_ENABLE_OFFLINE_MODE=true
VITE_CACHE_TIMEOUT_MS=30000
VITE_MAX_EVENT_LISTENERS=50
VITE_BATCH_DELAY_MS=100

# Debug Settings
VITE_ENABLE_WS_DEBUG=false
VITE_SHOW_PERFORMANCE_METRICS=false
